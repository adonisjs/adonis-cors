{
  "name": "@adonisjs/cors",
  "version": "1.0.5",
  "description": "CORS middleware for Adonisjs",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "nyc npm run test:local",
    "test:win": "set FORCE_COLOR=true && node bin/index.js --win",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "adonisjs",
    "adonis-framework",
    "cors"
  ],
  "author": "virk",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/sink": "^1.0.13",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "japa": "^1.0.6",
    "japa-cli": "^1.0.1",
    "nyc": "^12.0.1",
    "semver": "^5.5.0",
    "standard": "^10.0.3"
  },
  "nyc": {
    "include": [
      "src",
      "test"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-cors.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-cors/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-cors#readme"
}
